; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="RogueDefense"
run/main_scene="res://Scenes/MainMenu/MainMenu.tscn"
config/features=PackedStringArray("4.1", "C#")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://Assets/icon.png"

[display]

window/size/viewport_width=1025
window/size/viewport_height=600
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"
window/handheld/orientation="sensor_landscape"

[dotnet]

project/assembly_name="RogueDefense"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Enemy"
2d_physics/layer_3="PlayerProjectile"

[mono]

project/assembly_name="RogueDefense"

[physics]

2d/default_gravity=0
common/enable_pause_aware_picking=true
2d/run_on_thread=2

[rendering]

environment/defaults/default_clear_color=Color(0.2, 0.2, 0.2, 1)
environment/defaults/default_environment="res://default_env.tres"
